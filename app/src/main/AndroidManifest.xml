<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tld.anamtastic.www.anamtastic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AvailableHardwareActivity"
            android:label="@string/title_available_hardware"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LanguageSelectActivity"
            android:label="@string/title_activity_module_select"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SkinDiagnosticActivity"
            android:label="@string/title_activity_skin_diagnostic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ModuleSelectActivity"></activity>
        <activity android:name=".TimeOfOnsetActivity"></activity>
        <activity android:name=".StrokeSuspectActivity"></activity>
    </application>

</manifest>